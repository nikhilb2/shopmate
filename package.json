{
  "name": "nextjs",
  "version": "4.0.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.0.1",
    "@material-ui/icons": "^4.0.1",
    "@material-ui/styles": "^4.0.1",
    "@sentry/browser": "^5.4.0",
    "clsx": "^1.0.4",
    "isomorphic-unfetch": "^3.0.0",
    "js-cookie": "^2.2.0",
    "moment": "^2.29.4",
    "next": "^9.3.2",
    "next-cookies": "^1.1.2",
    "next-ga": "^2.3.4",
